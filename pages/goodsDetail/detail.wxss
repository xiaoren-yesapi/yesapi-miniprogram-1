  white-space: nowrap;
  padding: 0.4rem 0.65rem;
  margin-right: 0.25rem;
  background: #fff;
  border: #e7b801 solid 1px;
  font-size: 0.7rem;
  border-radius: 0.15rem;
  color: #333;
  display: inline-block;
  margin-bottom: 0.3rem;
}

.join_shopping_cart_view .select_class .category .cls_view .cls.cur {
  background: #e7b801;
  color: #fff;
}

.join_shopping_cart_view .select_class .category .cls_detail_view {
  border: #ccc solid 1px;
  border-radius: 0.35rem;
  width: 100%;
  overflow: hidden;
  display: flex;
}

.join_shopping_cart_view .select_class .category .cls_detail_view .cls_detail_text {
  font-size: 0.6rem;
  text-align: center;
  border-right: #ccc solid 1px;
  padding: 0.65rem 0.25rem;
  flex: 1;
}

.join_shopping_cart_view .select_class .category .cls_detail_view .cls_detail_text:last-child {
  border-right: none;
}

.join_shopping_cart_view .select_class .category .cls_detail_view .cls_detail_text .text {
  display: block;
  margin-top: 0.3rem;
}

.join_shopping_cart_view .select_class .category .sele_num {
