  width: 6rem;
}

.join_shopping_cart_view .select_class .category .sele_num .btn {
  height: 1.5rem;
  line-height: 1.5rem;
  width: 1.5rem;
  background-color: #fff;
  border: #ccc solid 1px;
  text-align: center;
  font-size: 0.6rem;
  color: #333;
  display: block;
}

.join_shopping_cart_view .select_class .category .sele_num .btn.add {
  border-top-right-radius: 0.25rem;
  border-bottom-right-radius: 0.25rem;
  border-left: none;
}

.join_shopping_cart_view .select_class .category .sele_num .hideInput {
  border: #ccc solid 1px;
  text-align: center;
  font-size: 0.8rem;
  color: #333;
  width: 3rem;
  height: 1.5rem;
}

.join_shopping_cart_view .select_class .category .sele_num .btn.minus {
  border-top-left-radius: 0.25rem;
  border-bottom-left-radius: 0.25rem;
  border-right: none;
