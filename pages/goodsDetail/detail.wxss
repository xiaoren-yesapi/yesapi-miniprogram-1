.join_shopping_cart_view {
  transform: translate3d(100%, 0, 0);
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 2;
  background: #fff;
  display: flex;
  flex-direction: column;
}

.join_shopping_cart_view .goods_info {
  background: #fff;
  margin-bottom: 0.25rem;
  padding: 0.5rem 0.75rem;
  border-top: #f1f1f1 solid 1px;
  display: flex;
  border-top: #f2f2f2 solid 0.5rem;
  border-bottom: #f2f2f2 solid 1px;
  position: relative;
  overflow: hidden;
}

.join_shopping_cart_view .goods_info .img_view {
  min-width: 3.5rem;
  min-height: 3.5rem;
  position: relative;
  padding: 0;
}

.join_shopping_cart_view .goods_info .img_view .img {
  width: 100%;
  height: 100%;
}

.join_shopping_cart_view .goods_info .item_info {
  position: relative;
  flex: 1;
  font-size: 0.6rem;
  margin-left: 0.75rem;
}

.join_shopping_cart_view .goods_info .item_info .title {
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  -moz-line-clamp: 1;
  -moz-box-orient: vertical;
  -ms-line-clamp: 1;
  -ms-box-orient: vertical;
  -o-line-clamp: 1;
  -o-box-orient: vertical;
  line-clamp: 1;
  box-orient: vertical;
  display: -webkit-box;
  width: 100%;
}

.join_shopping_cart_view .goods_info .item_info .selece_text_flag {
  color: #f1e63b;
  display: block;
  margin-top: 0.3rem;
}

.join_shopping_cart_view .goods_info .item_info .row {
  display: flex;
  color: #a9a8a8;
}

.join_shopping_cart_view .goods_info .item_info .row .price {
  color: #f1e63b;
}

.join_shopping_cart_view .goods_info .item_info .row .text {
  flex: 1;
  margin-top: 0.3rem;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.join_shopping_cart_view .goods_info .item_info .row .text:first-child {
  text-align: left;
}

.join_shopping_cart_view .goods_info .item_info .row .text:last-child {
  text-align: right;
}

.join_shopping_cart_view .cart_btns {
  display: flex;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}

.join_shopping_cart_view .cart_btns .buy_btn {
  margin: 0.5rem;
  -webkit-box-flex: 1;
  display: block;
  width: 100%;
  font-size: 0.7rem;
  text-align: center;
  height: 1.75rem;
  line-height: 1.75rem;
  color: #fff;
  border-radius: 0.25rem;
}

.join_shopping_cart_view .cart_btns .buy_btn.join {
  background: #000;
}

.join_shopping_cart_view .cart_btns .buy_btn.buy {
  background: #f1e63b;
}

.join_shopping_cart_view .goods_tip {
  padding: 0.4rem;
  border-radius: 0.25rem;
  width: 90%;
  margin: 0 auto;
  background-color: #f2f2f2;
  position: relative;
  overflow: hidden;
  color: #333;
  font-size: 0.6rem;
  line-height: 0.8rem;
}

.join_shopping_cart_view .goods_tip:before {
  content: '';
  position: absolute;
  top: -1.4rem;
  left: -1.6rem;
  border-color: transparent;
  border-width: 0.75rem 0.75rem;
  border-right-color: #e7b801;
  border-style: solid;
  width: 0.75rem;
  height: 0.75rem;
  z-index: 9999;
  transform: rotate(45deg);
}

.join_shopping_cart_view .select_class {
  padding: 0px 0.5rem 0rem 0.5rem;
  width: 100%;
  box-sizing: border-box;
}

.join_shopping_cart_view .select_class .category {
  padding-bottom: 1rem;
  border-bottom: #e6e6e6 solid 1px;
}

.join_shopping_cart_view .select_class .category .name {
  color: #333;
  font-size: 0.8rem;
  margin: 0.5rem 0;
  display: block;
}

